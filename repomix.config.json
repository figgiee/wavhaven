{
  "output": {
    "filePath": "repomix-output.txt.md",
    "style": "markdown",
    "compress": false,
    "removeComments": false,
    "removeEmptyLines": true
  },
  "include": [
    "src/**",
    "supabase/**",
    "prisma/**",
    "scripts/**",
    "lib/**",
    "next.config.*",
    "tailwind.config.ts",
    "tsconfig.json",
    "package.json",
    "components.json",
    "middleware.ts",
    "biome.json",
    "jest.config.js",
    "playwright.config.ts",
    "vitest.config.ts",
    "postcss.config.mjs",
    "pnpm-workspace.yaml",
    "README.md"
  ],
  "ignore": {
    "useDefaultPatterns": false,
    "useGitignore": true,
    "customPatterns": [
      "node_modules/**",
      ".next/**",
      "dist/**",
      "build/**",
      "out/**",
      ".git/**",
      "**/*.{mp3,wav,aac,ogg,flac,m4a,png,jpg,jpeg,gif,svg,ico,webp}",
      "**/*.{mp4,webm,avi,mov,mkv}",
      "**/*.{pdf,doc,docx,xls,xlsx}",
      "**/*.{woff,woff2,ttf,eot}",
      "**/*.{zip,tar,gz,rar,7z}",
      "problems.md",
      "migrationstuff.md",
      "implementation-plan.md",
      "wavhaven-tech-spec.md",
      "page-fixed.tsx",
      "pnpm-lock.yaml",
      "package-lock.json",
      ".env*",
      "tests-e2e/**",
      "test-results/**",
      "playwright-report/**",
      "playwright-screenshots/**",
      "wavhaven-docking/**",
      "react-email-starter/**",
      "refactoring/**",
      "repomix-output.txt.md",
      "*.log",
      "logs/**",
      ".idea/**",
      ".vscode/**",
      ".cursor/**",
      ".repomix/**",
      "*.sublime-*",
      ".DS_Store",
      "Thumbs.db",
      "tmp/**",
      ".cache/**",
      "*.pt",
      "*.onnx",
      "*.dat",
      "*.whl",
      "*.pth",
      "*.safetensors",
      "*.bin",
      "*.ckpt",
      "*.hdf5",
      "*.h5",
      "*.pkl",
      "*.joblib",
      "*.npy",
      "*.npz",
      "**/data/**",
      "**/models/**",
      "**/checkpoints/**",
      "**/output/**",
      "**/results/**",
      "**/venv/**",
      "**/.venv/**",
      "**/env/**",
      "**/__pycache__/**",
      "public/coverart/**",
      "storage_backup_wavhaven_tracks/**",
      "wavhaven_backup_*.backup",
      "*.backup"
    ]
  }
}