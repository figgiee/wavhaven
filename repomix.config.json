{
  "output": {
    "filePath": "repomix-output.txt.md",
    "style": "markdown",
    "compress": true,
    "removeComments": true,
    "removeEmptyLines": true
  },
  "include": [
    "app/**",
    "src/**",
    "store/**",
    "supabase/**",
    "prisma/**",
    "public/essentia/**",
    "scripts/**",
    "next.config.*",
    "tailwind.config.ts",
    "tsconfig.json",
    "package.json",
    "components.json",
    "middleware.ts",
    ".env.example",
    "essentialog.txt"
  ],
  "ignore": {
    "useDefaultPatterns": false,
    "useGitignore": true,
    "customPatterns": [
      "node_modules/**",
      ".next/**",
      "dist/**",
      "build/**",
      "out/**",
      ".git/**",
      "**/*.{mp3,wav,png,jpg,jpeg,gif,svg,ico,webp}",
      "**/*.{mp4,webm,avi,mov}",
      "**/*.{ogg}",
      "**/*.{pdf,doc,docx,xls,xlsx}",
      "**/*.{woff,ttf,eot}",
      "problems.md",
      "migrationstuff.md",
      "implementation-plan.md",
      "wavhaven-tech-spec.md",
      "page-fixed.tsx",
      "pnpm-lock.yaml",
      ".env*",
      "tests-e2e/**",
      "test-results/**",
      "playwright-report/**",
      "wavhaven-docking/**",
      "react-email-starter/**",
      "*.log",
      "logs/**",
      ".idea/**",
      "*.sublime-*",
      ".DS_Store",
      "Thumbs.db",
      "tmp/**",
      ".cache/**",
      "*.pt",
      "*.onnx",
      "*.dat",
      "*.whl",
      "*.pth",
      "*.zip",
      "*.safetensors",
      "*.bin",
      "*.ckpt",
      "*.hdf5",
      "*.h5",
      "*.pkl",
      "*.joblib",
      "*.npy",
      "*.npz",
      "**/data/**",
      "**/models/**",
      "**/checkpoints/**",
      "**/output/**",
      "**/results/**",
      "**/venv/**",
      "**/.venv/**",
      "**/env/**",
      "**/__pycache__/**"
    ]
  }
}